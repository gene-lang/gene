# Fibonacci benchmark - compute fib(24)
(var calls 0)

(fn fib [n]
  (set calls (+ calls 1))
  (if (<= n 1)
    n
    (+ (fib (- n 1)) (fib (- n 2)))))

(var start (nano-time))
(var result (fib 24))
(var end (nano-time))

(println "fib(24) = " result)
(println "Function calls: " calls)
(println "Time taken: " (/ (- end start) 1000000000.0) " seconds")