# Loop and arithmetic benchmark
(fn sum_loop [n]
  (var total 0)
  (var i 1)
  (while (<= i n)
    (total = (+ total i))
    (i = (+ i 1)))
  total)

(fn count_loop [n]
  (var count 0)
  (while (< count n)
    (count = (+ count 1)))
  count)

# Run benchmarks
(var n 1000)
(println "Loop benchmarks with n=" n)

(var result (sum_loop n))
(println "sum_loop: " result)

(var result2 (count_loop n))
(println "count_loop: " result2)