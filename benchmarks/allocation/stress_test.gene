# Simplified Allocation Stress Test

(fn create_simple_tree [depth]
  (if (<= depth 0)
    []
    [(create_simple_tree (- depth 1))
     (create_simple_tree (- depth 1))]))

(fn create_arrays [n]
  (var count 0)
  (var i 0)
  (while (< i n)
    (var arr [1 2 3 4 5])
    (count = (+ count 1))
    (i = (+ i 1)))
  count)

(fn create_maps [n]
